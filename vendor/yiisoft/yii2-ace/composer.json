{
    "name": "yiisoft/yii2-ace",
    "description": "The ace extension for the Yii framework",
    "keywords": ["yii2", "bootstrap","ace"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-bootstrap/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2-bootstrap"
    },
    "authors": [
        {
            "name": "joson",
            "email": "784844602@qq.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.6",
        "bower-asset/bootstrap": "3.3.* | 3.2.* | 3.1.*"
    },
    "autoload": {
        "psr-4": {
            "yii\\ace\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
